    "url": "https://github.com/cr4yfish/subnetter-wasm/issues"
  },
  "homepage": "https://github.com/cr4yfish/subnetter-wasm#readme",
  "dependencies": {
    "@nextui-org/react": "^1.0.0-beta.9",
    "@wasm-tool/wasm-pack-plugin": "^1.6.0",
    "next": "^12.1.6",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.5.4",
    "next-themes": "^0.2.0",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
