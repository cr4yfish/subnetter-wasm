    "build-rust": "rustc --target wasm32-unknown-unknown -O --crate-type=cdylib src/lib.rs -o subnetter.wasm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cr4yfish/subnetter-wasm.git"
  },
